<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/srilanka/travel/ontology#"
     xml:base="http://www.semanticweb.org/srilanka/travel/ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <!-- Ontology Header -->
    <owl:Ontology rdf:about="http://www.semanticweb.org/srilanka/travel/ontology">
        <rdfs:comment>Ontology representing the tourism domain in Sri Lanka.</rdfs:comment>
    </owl:Ontology>

    <!-- Classes -->
    <!-- Destination and its Subclasses -->
    <owl:Class rdf:about="#Destination"/>
    <owl:Class rdf:about="#City">
        <rdfs:subClassOf rdf:resource="#Destination"/>
    </owl:Class>
    <owl:Class rdf:about="#Beach">
        <rdfs:subClassOf rdf:resource="#Destination"/>
    </owl:Class>
    <owl:Class rdf:about="#HillCountry">
        <rdfs:subClassOf rdf:resource="#Destination"/>
    </owl:Class>

    <!-- Attraction and its Subclasses -->
    <owl:Class rdf:about="#Attraction"/>
    <owl:Class rdf:about="#CulturalSite">
        <rdfs:subClassOf rdf:resource="#Attraction"/>
    </owl:Class>
    <owl:Class rdf:about="#NaturalSite">
        <rdfs:subClassOf rdf:resource="#Attraction"/>
    </owl:Class>

    <!-- Accommodation and its Subclasses -->
    <owl:Class rdf:about="#Accommodation"/>
    <owl:Class rdf:about="#Hotel">
        <rdfs:subClassOf rdf:resource="#Accommodation"/>
    </owl:Class>
    <owl:Class rdf:about="#GuestHouse">
        <rdfs:subClassOf rdf:resource="#Accommodation"/>
    </owl:Class>

    <!-- Transportation and its Subclasses -->
    <owl:Class rdf:about="#Transportation"/>
    <owl:Class rdf:about="#Bus">
        <rdfs:subClassOf rdf:resource="#Transportation"/>
    </owl:Class>
    <owl:Class rdf:about="#Train">
        <rdfs:subClassOf rdf:resource="#Transportation"/>
    </owl:Class>
    <owl:Class rdf:about="#TukTuk">
        <rdfs:subClassOf rdf:resource="#Transportation"/>
    </owl:Class>

    <!-- Activity and its Subclasses -->
    <owl:Class rdf:about="#Activity"/>
    <owl:Class rdf:about="#Surfing">
        <rdfs:subClassOf rdf:resource="#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="#Hiking">
        <rdfs:subClassOf rdf:resource="#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="#WildlifeSafari">
        <rdfs:subClassOf rdf:resource="#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="#AyurvedaTreatment">
        <rdfs:subClassOf rdf:resource="#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="#TeaPlantationTour">
        <rdfs:subClassOf rdf:resource="#Activity"/>
    </owl:Class>

    <!-- Tourist Class -->
    <owl:Class rdf:about="#Tourist"/>

    <!-- Object Properties -->
    <!-- Destination-to-Attraction -->
    <owl:ObjectProperty rdf:about="#hasAttraction">
        <rdfs:domain rdf:resource="#Destination"/>
        <rdfs:range rdf:resource="#Attraction"/>
    </owl:ObjectProperty>

    <!-- Destination-to-Activity -->
    <owl:ObjectProperty rdf:about="#offersActivity">
        <rdfs:domain rdf:resource="#Destination"/>
        <rdfs:range rdf:resource="#Activity"/>
    </owl:ObjectProperty>

    <!-- Tourist-to-Activity -->
    <owl:ObjectProperty rdf:about="#preferredActivity">
        <rdfs:domain rdf:resource="#Tourist"/>
        <rdfs:range rdf:resource="#Activity"/>
    </owl:ObjectProperty>

    <!-- Attraction/Accommodation-to-Destination -->
    <owl:ObjectProperty rdf:about="#locatedIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Attraction"/>
                    <rdf:Description rdf:about="#Accommodation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="#Destination"/>
    </owl:ObjectProperty>

    <!-- Accommodation-to-Transportation -->
    <owl:ObjectProperty rdf:about="#providesTransportation">
        <rdfs:domain rdf:resource="#Accommodation"/>
        <rdfs:range rdf:resource="#Transportation"/>
    </owl:ObjectProperty>

    <!-- Recommendation Properties -->
    <owl:ObjectProperty rdf:about="#recommends">
        <rdfs:domain rdf:resource="#Tourist"/>
        <rdfs:range rdf:resource="#Destination"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#recommendsAccommodation">
        <rdfs:domain rdf:resource="#Tourist"/>
        <rdfs:range rdf:resource="#Accommodation"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <!-- hasName -->
    <owl:DatatypeProperty rdf:about="#hasName">
        <rdfs:domain rdf:resource="owl:Thing"/>
        <rdfs:range rdf:resource="xsd:string"/>
    </owl:DatatypeProperty>

    <!-- Accommodation Properties -->
    <owl:DatatypeProperty rdf:about="#hasRating">
        <rdfs:domain rdf:resource="#Accommodation"/>
        <rdfs:range rdf:resource="xsd:float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#hasCost">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Accommodation"/>
                    <rdf:Description rdf:about="#Activity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="xsd:float"/>
    </owl:DatatypeProperty>

    <!-- Activity Property -->
    <owl:DatatypeProperty rdf:about="#hasDuration">
        <rdfs:domain rdf:resource="#Activity"/>
        <rdfs:range rdf:resource="xsd:float"/>
    </owl:DatatypeProperty>

    <!-- Individuals -->
    <!-- Destinations -->
    <owl:NamedIndividual rdf:about="#Colombo">
        <rdf:type rdf:resource="#City"/>
        <hasName>Colombo</hasName>
        <offersActivity rdf:resource="#AyurvedaTreatmentActivity"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Kandy">
        <rdf:type rdf:resource="#City"/>
        <hasName>Kandy</hasName>
        <offersActivity rdf:resource="#CulturalTourActivity"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Ella">
        <rdf:type rdf:resource="#HillCountry"/>
        <hasName>Ella</hasName>
        <offersActivity rdf:resource="#HikingActivity"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Mirissa">
        <rdf:type rdf:resource="#Beach"/>
        <hasName>Mirissa</hasName>
        <offersActivity rdf:resource="#SurfingActivity"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#NuwaraEliya">
        <rdf:type rdf:resource="#HillCountry"/>
        <hasName>Nuwara Eliya</hasName>
        <offersActivity rdf:resource="#TeaPlantationTourActivity"/>
    </owl:NamedIndividual>

    <!-- Attractions -->
    <owl:NamedIndividual rdf:about="#TempleOfTheToothRelic">
        <rdf:type rdf:resource="#CulturalSite"/>
        <hasName>Temple of the Tooth Relic</hasName>
        <locatedIn rdf:resource="#Kandy"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#NineArchesBridge">
        <rdf:type rdf:resource="#NaturalSite"/>
        <hasName>Nine Arches Bridge</hasName>
        <locatedIn rdf:resource="#Ella"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#HortonPlains">
        <rdf:type rdf:resource="#NaturalSite"/>
        <hasName>Horton Plains</hasName>
        <locatedIn rdf:resource="#NuwaraEliya"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#WhaleWatching">
        <rdf:type rdf:resource="#NaturalSite"/>
        <hasName>Whale Watching</hasName>
        <locatedIn rdf:resource="#Mirissa"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#GalleFaceGreen">
        <rdf:type rdf:resource="#NaturalSite"/>
        <hasName>Galle Face Green</hasName>
        <locatedIn rdf:resource="#Colombo"/>
    </owl:NamedIndividual>

    <!-- Activities -->
    <owl:NamedIndividual rdf:about="#SurfingActivity">
        <rdf:type rdf:resource="#Surfing"/>
        <hasName>Surfing Activity</hasName>
        <hasCost rdf:datatype="xsd:float">25.0</hasCost>
        <hasDuration rdf:datatype="xsd:float">2.0</hasDuration>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#HikingActivity">
        <rdf:type rdf:resource="#Hiking"/>
        <hasName>Hiking Activity</hasName>
        <hasCost rdf:datatype="xsd:float">15.0</hasCost>
        <hasDuration rdf:datatype="xsd:float">4.0</hasDuration>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#TeaPlantationTourActivity">
        <rdf:type rdf:resource="#TeaPlantationTour"/>
        <hasName>Tea Plantation Tour Activity</hasName>
        <hasCost rdf:datatype="xsd:float">10.0</hasCost>
        <hasDuration rdf:datatype="xsd:float">3.0</hasDuration>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#AyurvedaTreatmentActivity">
        <rdf:type rdf:resource="#AyurvedaTreatment"/>
        <hasName>Ayurveda Treatment Activity</hasName>
        <hasCost rdf:datatype="xsd:float">50.0</hasCost>
        <hasDuration rdf:datatype="xsd:float">1.5</hasDuration>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#CulturalTourActivity">
        <rdf:type rdf:resource="#Activity"/>
        <hasName>Cultural Tour Activity</hasName>
        <hasCost rdf:datatype="xsd:float">30.0</hasCost>
        <hasDuration rdf:datatype="xsd:float">5.0</hasDuration>
    </owl:NamedIndividual>

    <!-- Accommodations -->
    <owl:NamedIndividual rdf:about="#ColomboCityHotel">
        <rdf:type rdf:resource="#Hotel"/>
        <hasName>Colombo City Hotel</hasName>
        <locatedIn rdf:resource="#Colombo"/>
        <hasRating rdf:datatype="xsd:float">4.2</hasRating>
        <hasCost rdf:datatype="xsd:float">80.0</hasCost>
        <providesTransportation rdf:resource="#TukTukService"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#EllaMountainGuestHouse">
        <rdf:type rdf:resource="#GuestHouse"/>
        <hasName>Ella Mountain Guest House</hasName>
        <locatedIn rdf:resource="#Ella"/>
        <hasRating rdf:datatype="xsd:float">4.6</hasRating>
        <hasCost rdf:datatype="xsd:float">50.0</hasCost>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#NuwaraEliyaTeaBungalow">
        <rdf:type rdf:resource="#Hotel"/>
        <hasName>Nuwara Eliya Tea Bungalow</hasName>
        <locatedIn rdf:resource="#NuwaraEliya"/>
        <hasRating rdf:datatype="xsd:float">4.8</hasRating>
        <hasCost rdf:datatype="xsd:float">120.0</hasCost>
    </owl:NamedIndividual>

    <!-- Transportation -->
    <owl:NamedIndividual rdf:about="#TukTukService">
        <rdf:type rdf:resource="#TukTuk"/>
        <hasName>Tuk Tuk Service</hasName>
    </owl:NamedIndividual>

    <!-- Tourists -->
    <owl:NamedIndividual rdf:about="#Amara">
        <rdf:type rdf:resource="#Tourist"/>
        <hasName>Amara</hasName>
        <preferredActivity rdf:resource="#HikingActivity"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Nimal">
        <rdf:type rdf:resource="#Tourist"/>
        <hasName>Nimal</hasName>
        <preferredActivity rdf:resource="#AyurvedaTreatmentActivity"/>
    </owl:NamedIndividual>

    <!-- SWRL Rules -->
    <!-- Recommend Destination Rule -->
    <swrl:Imp rdf:ID="RecommendDestinationRule">
        <rdfs:comment>If a Tourist prefers an Activity, and a Destination offers that Activity, then the Destination is recommended to the Tourist.</rdfs:comment>
        <swrl:body>
            <swrl:AtomList>
                <rdf:first>
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#preferredActivity"/>
                        <swrl:argument1 rdf:resource="#?tourist"/>
                        <swrl:argument2 rdf:resource="#?activity"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:first>
                            <swrl:IndividualPropertyAtom>
                                <swrl:propertyPredicate rdf:resource="#offersActivity"/>
                                <swrl:argument1 rdf:resource="#?destination"/>
                                <swrl:argument2 rdf:resource="#?activity"/>
                            </swrl:IndividualPropertyAtom>
                        </rdf:first>
                        <rdf:rest rdf:resource="rdf:nil"/>
                    </swrl:AtomList>
                </rdf:rest>
            </swrl:AtomList>
        </swrl:body>
        <swrl:head>
            <swrl:AtomList>
                <rdf:first>
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#recommends"/>
                        <swrl:argument1 rdf:resource="#?tourist"/>
                        <swrl:argument2 rdf:resource="#?destination"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest rdf:resource="rdf:nil"/>
            </swrl:AtomList>
        </swrl:head>
    </swrl:Imp>

    <!-- Recommend Accommodation Rule -->
    <swrl:Imp rdf:ID="RecommendAccommodationRule">
        <rdfs:comment>If an Accommodation is located in a Destination that is recommended to a Tourist, then recommend the Accommodation to the Tourist.</rdfs:comment>
        <swrl:body>
            <swrl:AtomList>
                <rdf:first>
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#recommends"/>
                        <swrl:argument1 rdf:resource="#?tourist"/>
                        <swrl:argument2 rdf:resource="#?destination"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:first>
                            <swrl:IndividualPropertyAtom>
                                <swrl:propertyPredicate rdf:resource="#locatedIn"/>
                                <swrl:argument1 rdf:resource="#?accommodation"/>
                                <swrl:argument2 rdf:resource="#?destination"/>
                            </swrl:IndividualPropertyAtom>
                        </rdf:first>
                        <rdf:rest rdf:resource="rdf:nil"/>
                    </swrl:AtomList>
                </rdf:rest>
            </swrl:AtomList>
        </swrl:body>
        <swrl:head>
            <swrl:AtomList>
                <rdf:first>
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="#recommendsAccommodation"/>
                        <swrl:argument1 rdf:resource="#?tourist"/>
                        <swrl:argument2 rdf:resource="#?accommodation"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest rdf:resource="rdf:nil"/>
            </swrl:AtomList>
        </swrl:head>
    </swrl:Imp>

</rdf:RDF>


<!-- <?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/srilanka/travel/ontology#"
     xml:base="http://www.semanticweb.org/srilanka/travel/ontology"
     xmlns:ont="http://www.semanticweb.org/srilanka/travel/ontology#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Ontology Metadata -->
    <owl:Ontology rdf:about="http://www.semanticweb.org/srilanka/travel/ontology">
        <rdfs:comment>Comprehensive Ontology for Sri Lanka Tourism</rdfs:comment>
    </owl:Ontology>

    <!-- Classes -->
    <!-- Destinations -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#City">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Beach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#HillCountry">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    </owl:Class>

    <!-- Attractions -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Attraction"/>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#CulturalSite">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Attraction"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#NaturalSite">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Attraction"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#WildlifePark">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#NaturalSite"/>
    </owl:Class>

    <!-- Accommodations -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Hotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#GuestHouse">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
    </owl:Class>

    <!-- Transportation -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Bus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Train">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Taxi">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#TukTuk">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    </owl:Class>

    <!-- Activities -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Surfing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Hiking">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#WildlifeSafari">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#AyurvedaTreatment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:Class>
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#TeaPlantationTour">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:Class>

    <!-- Tourists -->
    <owl:Class rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Tourist"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#hasAttraction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Attraction"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#offersActivity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#preferredActivity">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Tourist"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#locatedIn">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Attraction"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#providesTransportation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#hasRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#hasCost">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#Transportation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#hasDuration">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Activity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Duration in hours</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Added hasName Data Property -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#hasName">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- SWRL Rules -->
    <!-- Object Property for Recommendation -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#recommends">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Tourist"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Destination"/>
    </owl:ObjectProperty>

    <!-- SWRL Rule Definition -->
    <!-- [Tourist(?t) ^ preferredActivity(?t, ?a) ^ offersActivity(?d, ?a) -> recommends(?t, ?d)] -->
    <swrl:Imp>
        <rdfs:label>Recommend Destination Rule</rdfs:label>
        <swrl:body>
            <swrl:AtomList>
                <rdf:first>
                    <!-- Tourist(?t) -->
                    <swrl:ClassAtom>
                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Tourist"/>
                        <swrl:argument1 rdf:resource="urn:swrl#t"/>
                    </swrl:ClassAtom>
                </rdf:first>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:first>
                            <!-- preferredActivity(?t, ?a) -->
                            <swrl:IndividualPropertyAtom>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#preferredActivity"/>
                                <swrl:argument1 rdf:resource="urn:swrl#t"/>
                                <swrl:argument2 rdf:resource="urn:swrl#a"/>
                            </swrl:IndividualPropertyAtom>
                        </rdf:first>
                        <rdf:rest>
                            <swrl:AtomList>
                                <rdf:first>
                                    <!-- offersActivity(?d, ?a) -->
                                    <swrl:IndividualPropertyAtom>
                                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#offersActivity"/>
                                        <swrl:argument1 rdf:resource="urn:swrl#d"/>
                                        <swrl:argument2 rdf:resource="urn:swrl#a"/>
                                    </swrl:IndividualPropertyAtom>
                                </rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </swrl:AtomList>
                        </rdf:rest>
                    </swrl:AtomList>
                </rdf:rest>
            </swrl:AtomList>
        </swrl:body>
        <swrl:head>
            <swrl:AtomList>
                <rdf:first>
                    <!-- recommends(?t, ?d) -->
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#recommends"/>
                        <swrl:argument1 rdf:resource="urn:swrl#t"/>
                        <swrl:argument2 rdf:resource="urn:swrl#d"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </swrl:AtomList>
        </swrl:head>
    </swrl:Imp>

    <!-- Additional SWRL Rule -->
    <!-- Rule: If an accommodation is located in a destination recommended to a tourist, recommend the accommodation to the tourist -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/srilanka/travel/ontology#recommendsAccommodation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Tourist"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#Accommodation"/>
    </owl:ObjectProperty>

    <!-- [recommends(?t, ?d) ^ locatedIn(?ac, ?d) -> recommendsAccommodation(?t, ?ac)] -->
    <swrl:Imp>
        <rdfs:label>Recommend Accommodation Rule</rdfs:label>
        <swrl:body>
            <swrl:AtomList>
                <rdf:first>
                    <!-- recommends(?t, ?d) -->
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#recommends"/>
                        <swrl:argument1 rdf:resource="urn:swrl#t"/>
                        <swrl:argument2 rdf:resource="urn:swrl#d"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest>
                    <swrl:AtomList>
                        <rdf:first>
                            <!-- locatedIn(?ac, ?d) -->
                            <swrl:IndividualPropertyAtom>
                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#locatedIn"/>
                                <swrl:argument1 rdf:resource="urn:swrl#ac"/>
                                <swrl:argument2 rdf:resource="urn:swrl#d"/>
                            </swrl:IndividualPropertyAtom>
                        </rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </swrl:AtomList>
                </rdf:rest>
            </swrl:AtomList>
        </swrl:body>
        <swrl:head>
            <swrl:AtomList>
                <rdf:first>
                    <!-- recommendsAccommodation(?t, ?ac) -->
                    <swrl:IndividualPropertyAtom>
                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/srilanka/travel/ontology#recommendsAccommodation"/>
                        <swrl:argument1 rdf:resource="urn:swrl#t"/>
                        <swrl:argument2 rdf:resource="urn:swrl#ac"/>
                    </swrl:IndividualPropertyAtom>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </swrl:AtomList>
        </swrl:head>
    </swrl:Imp>

